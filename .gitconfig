[user]
	name = Ã˜ystein Walle
	email = oystwa@gmail.com
[color]
	ui = auto
[diff]
	tool = vimdiff
[core]
	editor = vim </dev/tty
	excludesfile = /home/osse/.gitignore
	pager = less -FSRX
[log]
	decorate = true
[alias]
	ci = commit
	st = status
	s = status
	d = diff
	co = checkout
	br = branch
	fuckit = reset --hard
	lg = log --graph --branches --oneline --date-order
	lgg = log --graph --all --oneline --date-order
	lgup = log --graph --oneline --date-order HEAD @{u}
	sdiff = diff --staged
	amend = commit --amend -C HEAD
	rcont = rebase --continue
	dirdiff = difftool --tool vdwrap --dir-diff
[push]
	default = upstream
[sendemail]
	smtpencryption = tls
	smtpserver = smtp.gmail.com
	smtpuser = oystwa@gmail.com
	smtpserverport = 587
[difftool "vdwrap"]
	cmd = vdwrap $LOCAL $REMOTE
[merge]
	defaultToUpstream = true
